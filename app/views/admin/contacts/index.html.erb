<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>User Management</h1>
      <br>
      <div>
        <%= link_to 'Agency', admin_agencies_path %> |
        <%= link_to 'Category', admin_categories_path %> |
        <%= link_to 'Feedback', admin_contacts_path %> |
        <%= link_to 'User', admin_users_path %> |
      </div>
      <br>
      <table class="table table-hover">
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Status</th>
            <th scope="col">Actions</th>
          </tr>
        </thead>
        <tbody>
          <% @contacts.each do |contact| %>
          <tr>
            <td><%= contact.name %></td>
            <td><%= contact.email %></td>
            <td><%= contact.status.capitalize %></td>
            <td>
              <%= link_to 'Show', admin_contact_path(contact) %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
