<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>Agency User Management</h1>
      <br>
      <div>
        <%= link_to 'Agency', admin_agencies_path %> |
        <%= link_to 'Category', admin_categories_path %> |
        <%= link_to 'Feedback', admin_contacts_path %> |
        <%= link_to 'User', admin_users_path %> |
        <%= link_to 'Agency User', admin_agencyusers_path %>
      </div>
      <br>
      <div>
        <%= link_to 'New Agency User', new_admin_agencyuser_path, class: "btn btn-primary" %>
      </div>
      <br>
      <table class="table table-hover">
        <thead>
          <tr>
            <th scope="col">Username</th>
            <th scope="col">Agency Name</th>
            <th scope="col">Email</th>
            <th scope="col">Actions</th>
          </tr>
        </thead>
        <tbody>
          <% @agencyusers.each do |agencyuser| %>
          <tr>
            <td><%= agencyuser.username %></td>
            <td><%= agencyuser.agency.name %></td>
            <td><%= agencyuser.email %></td>
            <td>
              <%= link_to 'Edit', edit_admin_agencyuser_path(agencyuser) %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
