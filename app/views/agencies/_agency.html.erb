<div class="col-md-7 mt-3">
  <% @agencies.each do |agency| %>
    <div class="card py-1">
      <div class="card-body">
        <h4><%= link_to agency.name, agency_path(agency) %></h4>
        <p><h6><%= agency.street_name%>, Singapore <%=agency.postal_code%></h6></p>
        <h7><%= agency.opening_hours%></h7>
      </div>
    </div>
  <% end %>
</div>
